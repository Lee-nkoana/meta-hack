{
    "info": {
        "name": "Medical Records Bridge API",
        "description": "Complete API collection for Medical Records Bridge backend",
        "schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json"
    },
    "auth": {
        "type": "bearer",
        "bearer": [
            {
                "key": "token",
                "value": "{{access_token}}",
                "type": "string"
            }
        ]
    },
    "item": [
        {
            "name": "Health & Status",
            "item": [
                {
                    "name": "Health Check",
                    "request": {
                        "method": "GET",
                        "header": [],
                        "url": {
                            "raw": "{{base_url}}/health",
                            "host": [
                                "{{base_url}}"
                            ],
                            "path": [
                                "health"
                            ]
                        }
                    }
                },
                {
                    "name": "API Root",
                    "request": {
                        "method": "GET",
                        "header": [],
                        "url": {
                            "raw": "{{base_url}}/",
                            "host": [
                                "{{base_url}}"
                            ],
                            "path": [
                                ""
                            ]
                        }
                    }
                }
            ]
        },
        {
            "name": "Authentication",
            "item": [
                {
                    "name": "Register User",
                    "event": [
                        {
                            "listen": "test",
                            "script": {
                                "exec": [
                                    "if (pm.response.code === 201) {",
                                    "    var jsonData = pm.response.json();",
                                    "    pm.environment.set(\"user_id\", jsonData.id);",
                                    "}"
                                ]
                            }
                        }
                    ],
                    "request": {
                        "auth": {
                            "type": "noauth"
                        },
                        "method": "POST",
                        "header": [
                            {
                                "key": "Content-Type",
                                "value": "application/json"
                            }
                        ],
                        "body": {
                            "mode": "raw",
                            "raw": "{\n  \"email\": \"patient@example.com\",\n  \"username\": \"patient1\",\n  \"password\": \"secure123\",\n  \"full_name\": \"John Doe\"\n}"
                        },
                        "url": {
                            "raw": "{{base_url}}/api/auth/register",
                            "host": [
                                "{{base_url}}"
                            ],
                            "path": [
                                "api",
                                "auth",
                                "register"
                            ]
                        }
                    }
                },
                {
                    "name": "Login",
                    "event": [
                        {
                            "listen": "test",
                            "script": {
                                "exec": [
                                    "if (pm.response.code === 200) {",
                                    "    var jsonData = pm.response.json();",
                                    "    pm.environment.set(\"access_token\", jsonData.access_token);",
                                    "}"
                                ]
                            }
                        }
                    ],
                    "request": {
                        "auth": {
                            "type": "noauth"
                        },
                        "method": "POST",
                        "header": [
                            {
                                "key": "Content-Type",
                                "value": "application/x-www-form-urlencoded"
                            }
                        ],
                        "body": {
                            "mode": "urlencoded",
                            "urlencoded": [
                                {
                                    "key": "username",
                                    "value": "patient1"
                                },
                                {
                                    "key": "password",
                                    "value": "secure123"
                                }
                            ]
                        },
                        "url": {
                            "raw": "{{base_url}}/api/auth/login",
                            "host": [
                                "{{base_url}}"
                            ],
                            "path": [
                                "api",
                                "auth",
                                "login"
                            ]
                        }
                    }
                },
                {
                    "name": "Get Current User",
                    "request": {
                        "method": "GET",
                        "header": [],
                        "url": {
                            "raw": "{{base_url}}/api/auth/me",
                            "host": [
                                "{{base_url}}"
                            ],
                            "path": [
                                "api",
                                "auth",
                                "me"
                            ]
                        }
                    }
                }
            ]
        },
        {
            "name": "Medical Records",
            "item": [
                {
                    "name": "Create Medical Record",
                    "event": [
                        {
                            "listen": "test",
                            "script": {
                                "exec": [
                                    "if (pm.response.code === 201) {",
                                    "    var jsonData = pm.response.json();",
                                    "    pm.environment.set(\"record_id\", jsonData.id);",
                                    "}"
                                ]
                            }
                        }
                    ],
                    "request": {
                        "method": "POST",
                        "header": [
                            {
                                "key": "Content-Type",
                                "value": "application/json"
                            }
                        ],
                        "body": {
                            "mode": "raw",
                            "raw": "{\n  \"title\": \"Blood Test Results\",\n  \"original_text\": \"WBC: 7.5, RBC: 4.8, HGB: 14.2, PLT: 250. All values within normal limits.\",\n  \"record_type\": \"lab_result\"\n}"
                        },
                        "url": {
                            "raw": "{{base_url}}/api/records",
                            "host": [
                                "{{base_url}}"
                            ],
                            "path": [
                                "api",
                                "records"
                            ]
                        }
                    }
                },
                {
                    "name": "List Medical Records",
                    "request": {
                        "method": "GET",
                        "header": [],
                        "url": {
                            "raw": "{{base_url}}/api/records?skip=0&limit=10",
                            "host": [
                                "{{base_url}}"
                            ],
                            "path": [
                                "api",
                                "records"
                            ],
                            "query": [
                                {
                                    "key": "skip",
                                    "value": "0"
                                },
                                {
                                    "key": "limit",
                                    "value": "10"
                                }
                            ]
                        }
                    }
                },
                {
                    "name": "Get Medical Record",
                    "request": {
                        "method": "GET",
                        "header": [],
                        "url": {
                            "raw": "{{base_url}}/api/records/{{record_id}}",
                            "host": [
                                "{{base_url}}"
                            ],
                            "path": [
                                "api",
                                "records",
                                "{{record_id}}"
                            ]
                        }
                    }
                },
                {
                    "name": "Update Medical Record",
                    "request": {
                        "method": "PUT",
                        "header": [
                            {
                                "key": "Content-Type",
                                "value": "application/json"
                            }
                        ],
                        "body": {
                            "mode": "raw",
                            "raw": "{\n  \"title\": \"Blood Test Results - Updated\"\n}"
                        },
                        "url": {
                            "raw": "{{base_url}}/api/records/{{record_id}}",
                            "host": [
                                "{{base_url}}"
                            ],
                            "path": [
                                "api",
                                "records",
                                "{{record_id}}"
                            ]
                        }
                    }
                },
                {
                    "name": "Delete Medical Record",
                    "request": {
                        "method": "DELETE",
                        "header": [],
                        "url": {
                            "raw": "{{base_url}}/api/records/{{record_id}}",
                            "host": [
                                "{{base_url}}"
                            ],
                            "path": [
                                "api",
                                "records",
                                "{{record_id}}"
                            ]
                        }
                    }
                }
            ]
        },
        {
            "name": "AI Services",
            "item": [
                {
                    "name": "Translate Medical Text",
                    "request": {
                        "method": "POST",
                        "header": [
                            {
                                "key": "Content-Type",
                                "value": "application/json"
                            }
                        ],
                        "body": {
                            "mode": "raw",
                            "raw": "{\n  \"text\": \"Patient presents with acute myalgia and pyrexia. Administered antipyretics.\"\n}"
                        },
                        "url": {
                            "raw": "{{base_url}}/api/ai/translate",
                            "host": [
                                "{{base_url}}"
                            ],
                            "path": [
                                "api",
                                "ai",
                                "translate"
                            ]
                        }
                    }
                },
                {
                    "name": "Get Lifestyle Suggestions",
                    "request": {
                        "method": "POST",
                        "header": [
                            {
                                "key": "Content-Type",
                                "value": "application/json"
                            }
                        ],
                        "body": {
                            "mode": "raw",
                            "raw": "{\n  \"condition\": \"Patient has elevated cholesterol levels (LDL: 160)\"\n}"
                        },
                        "url": {
                            "raw": "{{base_url}}/api/ai/suggestions",
                            "host": [
                                "{{base_url}}"
                            ],
                            "path": [
                                "api",
                                "ai",
                                "suggestions"
                            ]
                        }
                    }
                },
                {
                    "name": "Explain Medical Record",
                    "request": {
                        "method": "POST",
                        "header": [],
                        "url": {
                            "raw": "{{base_url}}/api/ai/explain/{{record_id}}?force_refresh=false",
                            "host": [
                                "{{base_url}}"
                            ],
                            "path": [
                                "api",
                                "ai",
                                "explain",
                                "{{record_id}}"
                            ],
                            "query": [
                                {
                                    "key": "force_refresh",
                                    "value": "false"
                                }
                            ]
                        }
                    }
                }
            ]
        },
        {
            "name": "User Profile",
            "item": [
                {
                    "name": "Get User Profile",
                    "request": {
                        "method": "GET",
                        "header": [],
                        "url": {
                            "raw": "{{base_url}}/api/users/me",
                            "host": [
                                "{{base_url}}"
                            ],
                            "path": [
                                "api",
                                "users",
                                "me"
                            ]
                        }
                    }
                },
                {
                    "name": "Update User Profile",
                    "request": {
                        "method": "PUT",
                        "header": [
                            {
                                "key": "Content-Type",
                                "value": "application/json"
                            }
                        ],
                        "body": {
                            "mode": "raw",
                            "raw": "{\n  \"full_name\": \"John Doe Updated\",\n  \"email\": \"newemail@example.com\"\n}"
                        },
                        "url": {
                            "raw": "{{base_url}}/api/users/me",
                            "host": [
                                "{{base_url}}"
                            ],
                            "path": [
                                "api",
                                "users",
                                "me"
                            ]
                        }
                    }
                },
                {
                    "name": "Get Dashboard Statistics",
                    "request": {
                        "method": "GET",
                        "header": [],
                        "url": {
                            "raw": "{{base_url}}/api/users/dashboard",
                            "host": [
                                "{{base_url}}"
                            ],
                            "path": [
                                "api",
                                "users",
                                "dashboard"
                            ]
                        }
                    }
                }
            ]
        }
    ],
    "variable": [
        {
            "key": "base_url",
            "value": "http://localhost:8000",
            "type": "string"
        }
    ]
}